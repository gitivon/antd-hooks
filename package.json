{
  "private": true,
  "scripts": {
    "start": "umi dev",
    "build": "umi build",
    "postinstall": "umi generate tmp",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "test": "umi-test",
    "test:coverage": "umi-test --coverage"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@ant-design/icons": "^4.6.2",
    "@ant-design/pro-layout": "^5.0.12",
    "@umijs/preset-react": "1.x",
    "@umijs/test": "^3.2.28",
    "ahooks": "^2.10.0",
    "antd": "^4.15.0",
    "fast-deep-equal": "^3.1.3",
    "jquery": "^3.6.0",
    "lint-staged": "^10.0.7",
    "lodash.set": "^4.3.2",
    "mockjs": "^1.1.0",
    "prettier": "^1.19.1",
    "rc-field-form": "^1.20.0",
    "rc-form": "^2.4.12",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "umi": "^3.2.28",
    "yorkie": "^2.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.5",
    "@types/mockjs": "^1.0.3"
  }
}
